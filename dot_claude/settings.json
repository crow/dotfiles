{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node \"/Users/david/.claude/hooks/gsd-check-update.js\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "node \"/Users/david/.claude/hooks/gsd-statusline.js\""
  },
  "enabledPlugins": {
    "context7@claude-plugins-official": true,
    "greptile@claude-plugins-official": true,
    "swift-lsp@claude-plugins-official": true,
    "impeccable@impeccable": true
  },
  "alwaysThinkingEnabled": true,
  "skipDangerousModePermissionPrompt": true,
  "mcpServers": {
    "airship-docs": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/david/source/internal-airship-mcp/airship-docs",
        "airship-docs"
      ]
    },
    "airship-api": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/david/source/internal-airship-mcp/airship-api",
        "airship-api"
      ],
      "env": {
        "AIRSHIP_APP_KEY": "${AIRSHIP_DEV_APP_KEY}",
        "AIRSHIP_MASTER_SECRET": "${AIRSHIP_DEV_APP_MASTER_SECRET}",
        "AIRSHIP_API_URL": "https://go.urbanairship.com"
      }
    },
    "airship-custom-views": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/david/source/internal-airship-mcp/airship-custom-views",
        "airship-custom-views"
      ]
    },
    "airship-migration": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/david/source/internal-airship-mcp/airship_migration",
        "airship-migration"
      ]
    },
    "airship-sample-generator": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/david/source/internal-airship-mcp/airship-sample-generator",
        "airship-sample-generator"
      ]
    },
    "XcodeBuildMCP": {
      "command": "npx",
      "args": [
        "-y",
        "xcodebuildmcp@latest",
        "mcp"
      ]
    },
    "xcodeproj": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-v",
        "/Users/david/source:/workspace",
        "ghcr.io/giginet/xcodeproj-mcp-server",
        "/workspace"
      ]
    }
  }
}
